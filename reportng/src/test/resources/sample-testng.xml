<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="鱼骨自动化测试v1.0" verbose="1" thread-count="2">

  <test name="所有的测试" parallel="true">
    <packages>
      <package name="org.uncommons.reportng.sample.*" />
    </packages>
  </test>

  <!-- Use the same tests again. -->
  <test name="不成功的测试" >
    <classes>
      <class name="org.uncommons.reportng.sample.FailedTests">
      </class>
      <class name="org.uncommons.reportng.sample.SkippedTests" />
    </classes>
  </test>

</suite>
